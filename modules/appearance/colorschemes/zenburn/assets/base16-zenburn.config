! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #404040, #404040, #383838

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #404040, #dcdccc, #404040, #404040, #ffffff
rofi.color-active:  #404040, #7cb8bb, #404040, #404040, #7cb8bb
rofi.color-urgent:  #404040, #dca3a3, #404040, #404040, #dca3a3

! Set the desired separator style
rofi.separator-style: solid
